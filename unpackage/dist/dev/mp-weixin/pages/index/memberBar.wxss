.main {
  padding: 160rpx 0rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.main .title {
  text-align: center;
  color: #505050;
}
.main > image {
  margin: 60rpx 0;
  width: 460rpx;
  height: 460rpx;
}
.main .botton {
  display: flex;
  justify-content: space-around;
  width: 100%;
}
.main .botton image {
  width: 50rpx;
  height: 50rpx;
}
.main .botton image:nth-child(2) {
  width: 40rpx;
}

