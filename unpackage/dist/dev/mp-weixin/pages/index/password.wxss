.tab-bar {
  height: 30rpx;
  background-color: #FF9EC1;
}
.footer {
  width: 100%;
  position: absolute;
  bottom: 0;
  left: 0;
}
.footer navigator {
  text-align: center;
  padding: 20rpx;
  color: #fff;
  background-color: #FF9EC3;
}
.password {
  padding: 100rpx 40rpx;
  font-size: 32rpx;
  color: #505050;
}
.password .title {
  padding-left: 20rpx;
}
.password .new {
  position: relative;
  padding-top: 26rpx;
}
.password .new image {
  position: absolute;
  z-index: 1000;
  width: 50rpx;
  height: 20rpx;
  right: 10rpx;
  top: 88rpx;
}
.password .new .top {
  padding-left: 20rpx;
}
.password .new input {
  border: 1px solid #999;
  padding: 14rpx;
  border-radius: 20rpx;
  margin: 6rpx 0;
}
.password .new .tip {
  padding-left: 20rpx;
  color: #FF85B4;
  font-size: 30rpx;
}

