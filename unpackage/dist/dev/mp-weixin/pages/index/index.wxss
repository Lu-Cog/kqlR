.content {
  min-height: 100vh;
  background-color: #F8F8F8;
}
.header {
  background: url(data:image/png;base64,) no-repeat;
  background-position: bottom center;
  width: 100%;
  height: 300rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.header image {
  border-radius: 50%;
  position: relative;
  width: 216rpx;
  height: 216rpx;
  top: 180rpx;
  box-shadow: 0rpx 0rpx 8rpx 8rpx rgba(5, 4, 3, 0.2);
}
.header .name {
  position: relative;
  top: 180rpx;
}
.mian {
  padding: 180rpx 0;
}
.item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 60rpx;
  color: #6C6C6C;
  text-align: 24rpx;
  background-color: #fff;
  margin-bottom: 1px;
}
.item input {
  text-align: right;
  flex: 1;
}
.item .avatar {
  width: 60rpx;
  height: 60rpx;
}
.item image {
  width: 18rpx;
  height: 18rpx;
}
.mag {
  margin-top: 20rpx;
}

