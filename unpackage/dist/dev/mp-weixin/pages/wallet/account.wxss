.tab-bar {
  height: 30rpx;
  background-color: #FF9EC1;
}
.account_body {
  height: 100vh;
  background-color: #F8F8F8;
}
.footer {
  width: 100%;
  position: absolute;
  bottom: 0;
}
.footer navigator {
  text-align: center;
  padding: 20rpx;
  color: #fff;
  background-color: #FF9EC3;
}
.item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx 60rpx;
  color: #000;
  text-align: 24rpx;
  background-color: #fff;
  margin-bottom: 1px;
}
.item input {
  text-align: right;
  color: #000;
}
.item .avatar {
  width: 60rpx;
  height: 60rpx;
}
.item image {
  width: 18rpx;
  height: 18rpx;
}
.mag {
  margin-top: 20rpx;
}

