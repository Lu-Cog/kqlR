.tab-bar {
  height: 30rpx;
  background-color: #FF9EC1;
}
.footer {
  width: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 100;
}
.footer navigator {
  text-align: center;
  padding: 20rpx;
  color: #fff;
  background-color: #FF9EC3;
}
.img {
  position: absolute;
  right: 30rpx;
}
.img image {
  width: 288rpx;
  height: 130rpx;
}
.bg_img {
  width: 100%;
  box-sizing: border-box;
  background: url(data:image/png;base64,) no-repeat;
  margin-top: -4rpx;
  background-size: 100% 100%;
  border-bottom: 1px solid #fff;
  letter-spacing: 2px;
  padding: 34rpx 30rpx;
  min-height: 80vh;
  position: relative;
  padding-bottom: 230rpx;
}
.bg_img .bottom {
  border-top: 1px dashed #707070;
  background-color: #fff;
  padding: 30rpx 44rpx;
  font-size: 32rpx;
  display: flex;
  align-items: center;
}
.bg_img .bottom text {
  font-size: 40rpx;
  font-weight: 700;
}
.bg_img .bottom input {
  width: 60rpx;
  text-align: center;
  background-color: #E8E8E8;
  font-size: 40rpx;
  font-weight: 700;
  margin: 0 20rpx;
}
.bg_img .main {
  background-color: #fff;
  padding: 30rpx 44rpx;
  font-size: 34rpx;
}
.bg_img .main view {
  margin-bottom: 20rpx;
}
.bg_img .main .info {
  font-weight: 700;
  margin-bottom: 30rpx;
}

