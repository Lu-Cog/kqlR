.mask {
  width: 100%;
  height: 100vh;
  position: fixed;
  z-index: 10000;
  top: 0;
  left: 0;
}
.mask .maskb {
  background: rgba(0, 0, 0, 0.2);
  width: 100%;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
}
.mask .tongzhi {
  background-color: #fff;
  z-index: 999;
  position: fixed;
  bottom: 6vh;
  left: 0;
  height: 40vh;
  width: 100%;
  border-top-left-radius: 50rpx;
  border-top-right-radius: 50rpx;
  padding: 50rpx 60rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}
.mask .tongzhi image {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 46;
  left: 64rpx;
}
.mask .tongzhi .top {
  color: #FF85B4;
  font-size: 40rpx;
}
.mask .tongzhi .tip {
  letter-spacing: 2px;
}
.mask .tongzhi textarea {
  width: 100%;
  background: #F1F1F1;
  border: 1px solid #C0C0C0;
  border-radius: 20rpx;
  padding: 20rpx;
  box-sizing: border-box;
}
.mask .maskbottom {
  z-index: 999;
  width: 100%;
  position: fixed;
  height: 6vh;
  line-height: 6vh;
  color: #fff;
  bottom: 0;
  left: 0;
  background: #FF85B4;
  text-align: center;
}
.header {
  background-color: #FF9EC3;
}
.header .top {
  height: 25px;
  width: 100%;
}
.header .head {
  padding: 30rpx;
