.header {
  background-color: #FF9EC3;
}
.header .top {
  height: 25px;
  width: 100%;
}
.header .head {
  padding: 30rpx;
  padding-top: 40rpx;
  color: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.header .head image:first-child {
  width: 22rpx;
  height: 22rpx;
}
.header .head image:last-child {
  width: 42rpx;
  height: 42rpx;
}

